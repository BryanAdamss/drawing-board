{
  "name": "@bryanadamss/drawing-board",
  "version": "1.0.2",
  "description": "canvas绘图板，提供绘制、撤销、旋转、下载等功能",
  "main": "dist/drawing-board.es.js",
  "module": "dist/drawing-board.es.js",
  "unpkg": "dist/drawing-board.umd.js",
  "scripts": {
    "clean": "shx rm -rf dist/*",
    "setNpmOrigin": "npm config set registry https://registry.npmjs.org/",
    "setTbOrigin": "npm config set registry https://registry.npm.taobao.org/",
    "prepublishOnly": "npm run build",
    "postpublish": "npm run setTbOrigin",
    "build": "npm run clean && rollup --config rollup.config.js",
    "release": "standard-version",
    "release:first": "npm run release --first-release",
    "publish2Npm": "npm run setNpmOrigin && git push --follow-tags origin master &&  && npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BryanAdamss/drawing-board.git"
  },
  "keywords": [
    "canvas",
    "drawing",
    "paint"
  ],
  "author": "GuangHui",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "core-js": "^3.2.1",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-babel-minify": "^9.0.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "shx": "^0.3.2",
    "standard-version": "^7.0.0"
  },
  "bugs": {
    "url": "https://github.com/BryanAdamss/drawing-board/issues"
  },
  "homepage": "https://github.com/BryanAdamss/drawing-board#readme",
  "dependencies": {}
}
